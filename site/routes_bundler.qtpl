{% func bundlerContent(manifest PluginManifest) %}

import { Datastar as DS } from "../engine"
{%- for _, p := range manifest.Plugins -%}
import { {%s p.Name %} } from "{%s p.Path %}"
{%- endfor -%}

DS.load(
{%- for _, p := range manifest.Plugins -%}
    {%s p.Name %},
{%- endfor -%}
)

export const Datastar = DS
{% endfunc %}